{
  "hosting": {
    "public": "dist/chicanacom/browser",
    "ignore": [
      "firebase.json",
      "*/.",
      "*/node_modules/*"
    ],
    "redirects": [
      {
        "source": "/",
        "destination": "/home",
        "type": 302
      }
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}